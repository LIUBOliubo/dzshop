page{
    height: 100%;
}
.myselfbox{
    width: 100%;
    height: 100%;
    display: flex;
    box-sizing: border-box;
    flex-direction: column;
    padding: 20rpx 20rpx;
    justify-content: flex-start;
    background: lightyellow;
    
}
.myself{
    width: 100%;
     height: auto;
      
     flex-direction: column;
     box-sizing: border-box;
     justify-content: flex-start;
     margin-top:40rpx;
     
}
.myself b{
    width: 100%;
    height:40rpx; 
    font-size: 34rpx;
    font-weight: 600;
    box-sizing: border-box;
}
.myself p{
    box-sizing: border-box;
    width: 100%;
    display: flex;
    padding: 30rpx 10rpx;
    padding: 20rpx 10rpx;
    font-size: 30rpx;
    line-height: 56rpx;
    border-radius: 20rpx;
    border: 4rpx solid #eee;
    background: lightgreen;
    color: #342342;
}

.myself1{
    width: 100%;
    height: 100rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin: 50rpx 0;
    font-size: 30rpx;
    
}